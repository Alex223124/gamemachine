ifndef config
  config=debug
endif
export config

PROJECTS := Detour detour_path pathfind_test

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

Detour: 
	@echo "==== Building Detour ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f Detour.make

detour_path: Detour
	@echo "==== Building detour_path ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f detour_path.make

pathfind_test: Detour
	@echo "==== Building pathfind_test ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f pathfind_test.make

clean:
	@${MAKE} --no-print-directory -C . -f Detour.make clean
	@${MAKE} --no-print-directory -C . -f detour_path.make clean
	@${MAKE} --no-print-directory -C . -f pathfind_test.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "   debug"
	@echo "   release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   Detour"
	@echo "   detour_path"
	@echo "   pathfind_test"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
